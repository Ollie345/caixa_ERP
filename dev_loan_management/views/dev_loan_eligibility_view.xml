<odoo>
    <record id="view_dev_loan_eligibility_form" model="ir.ui.view">
        <field name="name">view.dev.loan.eligibility.form</field>
        <field name="model">dev.loan.eligibility</field>
        <field name="arch" type="xml">
            <form string="Eligibility Criteria">
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                        </group>
                    </group>

                </sheet>
            </form>
        </field>
    </record>

    <record id="view_dev_loan_eligibility_tree" model="ir.ui.view">
        <field name="name">view.dev.loan.eligibility.tree</field>
        <field name="model">dev.loan.eligibility</field>
        <field name="arch" type="xml">
            <list string="Loan Eligibility Criteria" editable="bottom">
                <field name="name"/>
            </list>
        </field>
    </record>


    <record id="action_dev_loan_eligibility" model="ir.actions.act_window">
        <field name="name">Loan Eligibility Criteria</field>
        <field name="res_model">dev.loan.eligibility</field>
        <field name="view_mode">list,form</field>
    </record>

	<!--  Menu Items-->
    
    
    <menuitem name="Eligibility Criteria" id="menu_dev_loan_eligibility" parent="dev_loan_management.menu_dev_loan_configration" action="action_dev_loan_eligibility" sequence="4"/>
</odoo>
