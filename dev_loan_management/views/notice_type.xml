<odoo>

    <record id="view_ln_notice_type_tree" model="ir.ui.view">
        <field name="name">view.ln.notice.type.tree</field>
        <field name="model">ln.notice.type</field>
        <field name="arch" type="xml">
            <list string="Notice Type" editable="bottom">
                <field name="name" required='1'/>
            </list>
        </field>
    </record>

    <record id="action_ln_notice_type" model="ir.actions.act_window">
        <field name="name">Notice Type</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">ln.notice.type</field>
        <field name="view_mode">list</field>
    </record>

    <menuitem name="Notice Type" id="menu_ln_notice_type" parent='main_menu_loan_agreement_config'
              action="action_ln_notice_type" sequence='2'/>

 
              
</odoo>
