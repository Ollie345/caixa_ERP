<odoo>

    <record id="view_ln_agreement_type_tree" model="ir.ui.view">
        <field name="name">view.ln.agreement.type.tree</field>
        <field name="model">ln.agreement.type</field>
        <field name="arch" type="xml">
            <list string="Agreement Type" editable="bottom">
                <field name="name" required='1'/>
            </list>
        </field>
    </record>

    <record id="action_ln_agreement_type" model="ir.actions.act_window">
        <field name="name">Agreement Type</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">ln.agreement.type</field>
        <field name="view_mode">list</field>
    </record>

    <menuitem name="Agreement Type" id="menu_ln_agreement_type" parent='main_menu_loan_agreement_config'
              action="action_ln_agreement_type" sequence='1'/>
</odoo>

