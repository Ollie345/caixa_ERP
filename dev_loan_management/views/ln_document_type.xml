<odoo>
    <record id="view_ln_document_type_tree" model="ir.ui.view">
        <field name="name">view.ln.document.type.tree</field>
        <field name="model">ln.document.type</field>
        <field name="arch" type="xml">
            <list string="Loan Documents Type"  editable="bottom">
                <field name="name" required='1'/>
            </list>
        </field>
    </record>

    <record id="action_ln_document_type" model="ir.actions.act_window">
        <field name="name">Document Type</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">ln.document.type</field>
        <field name="view_mode">list</field>
    </record>

    <menuitem name="Document Type" id="menu_ln_document_type" parent='main_menu_dev_loan_document_checklist'
              action="action_ln_document_type" sequence='3'/>
</odoo>
