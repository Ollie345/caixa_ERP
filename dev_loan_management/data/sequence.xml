<odoo>
        
        <record id="dev_loan_sequence_number" model="ir.sequence">
            <field name="name">Loan Sequence</field>
            <field name="code">dev.loan.loan</field>
            <field name="prefix">LOAN/%(year)s/</field>
            <field name="padding">3</field>
        </record>

        <record id="dev_loan_agreement_sequence_number" model="ir.sequence">
            <field name="name">Loan Agreement Sequence</field>
            <field name="code">dev.loan.agreement</field>
            <field name="prefix">LA/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="dev_loan_notice_sequence_number" model="ir.sequence">
            <field name="name">Loan Notice Sequence</field>
            <field name="code">dev.loan.notice</field>
            <field name="prefix">LNTC/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <record id="dev_loan_advance_payment" model="ir.sequence">
            <field name="name">Advance Payment</field>
            <field name="code">dev.advance.payment</field>
            <field name="prefix">ADV/%(year)s/</field>
            <field name="padding">3</field>
        </record>
        
</odoo>
